{
    "name": "Business Time",
    "description": "Calculate business days since the selected date",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": ["tabs", "contextMenus"],
    "background": {
        "scripts": ["src/moment.min.js", "src/business.js"]
    },
    "commands": {
      "getBusinessDays": {
        "suggested_key": {
          "default": "Ctrl+Shift+X"
        },
        "description": "Get business days since date"
      }
    },
    "icons": {
        "48": "images/icon48.png"
    }

}
